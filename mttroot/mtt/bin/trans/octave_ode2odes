###############################################################
## Version control history
###############################################################
## $Id$
## $Log$
###############################################################


echo Creating $1_odes.dat2
$MATRIX -q <<EOF >octave_ode2odes.log
  global MTT_data
  $1_ode2odes;
  save -ascii $1_odes.dat2 MTT_data
EOF

